{
  "name": "generate-pebble-data",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "proto": "pbjs -t static-module -w es6 -o app/protogen/pebble.js  app/proto/*.proto && pbts -o app/protogen/pebble.d.ts app/protogen/pebble.js"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.9",
    "@chakra-ui/react": "1.6.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "2.28.0",
    "async-mqtt": "^2.6.1",
    "aws-crt": "^1.8.5",
    "aws-iot-device-sdk": "^2.2.10",
    "blitz": "0.38.6",
    "ethereumjs-util": "^7.0.10",
    "final-form": "4.20.2",
    "framer-motion": "^4",
    "lodash": "^4.17.21",
    "mobx": "^6.2.0",
    "mobx-react-lite": "^3.2.0",
    "number-precision": "^1.5.0",
    "protobufjs": "^6.11.2",
    "react": "alpha",
    "react-dom": "alpha",
    "react-final-form": "6.5.3",
    "react-hot-toast": "2.0.0",
    "sha3": "^2.1.4",
    "zod": "3.5.1"
  },
  "devDependencies": {
    "@types/aws-iot-device-sdk": "^2.2.3",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.15",
    "eslint": "7.31.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "prettier-plugin-prisma": "0.15.0",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.4",
    "typescript": "~4.3"
  },
  "private": true
}
